<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Client</display-name>

	<servlet>
  	<servlet-name>LogUtil</servlet-name>
  	<servlet-class>Controler.LogUtil</servlet-class> 
  </servlet>
  <servlet-mapping>
  	<servlet-name>LogUtil</servlet-name>
  	<url-pattern>/log/*</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>LogOut</servlet-name>
  	<servlet-class>Controler.LogOut</servlet-class> 
  </servlet>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout/*</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>NewUtil</servlet-name>
  	<servlet-class>Controler.NewUtil</servlet-class> 
  </servlet>
  <servlet-mapping>
  	<servlet-name>NewUtil</servlet-name>
  	<url-pattern>/newutil/*</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>AddManga</servlet-name>
  	<servlet-class>Controler.AddManga</servlet-class> 
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddManga</servlet-name>
  	<url-pattern>/addmanga/*</url-pattern>
  </servlet-mapping>
</web-app>